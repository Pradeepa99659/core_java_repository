package Atshaya;
import java.util.*;
public class Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a,b,c;
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the a and b values:");
		a=s.nextInt();
		b=s.nextInt();
		c=a + b;
		System.out.println("addition of a=" +a+ "and b=" +b+ "is:" +c);		
	}

}
